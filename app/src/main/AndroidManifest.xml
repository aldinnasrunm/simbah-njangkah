<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.ACTIVITY_RECOGNITION" />
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_simbah_njangkah"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_simbah_njangkah_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SimbahNjangkah"
        tools:targetApi="31">
        <activity
            android:name=".HistoryActivity"
            android:exported="false"
            android:label="@string/title_activity_history"
            android:theme="@style/Theme.SimbahNjangkah" />
        <activity
            android:name=".WelcomingActivity"
            android:exported="false"
            android:label="@string/title_activity_welcoming"
            android:theme="@style/Theme.SimbahNjangkah" />
        <activity
            android:name=".JangkahActivity"
            android:exported="false"
            android:label="@string/title_activity_jangkah"
            android:theme="@style/Theme.SimbahNjangkah" />
        <activity
            android:name=".Screening"
            android:exported="false"
            android:label="@string/title_activity_screening"
            android:theme="@style/Theme.SimbahNjangkah" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false"
            android:label="@string/title_activity_register"
            android:theme="@style/Theme.SimbahNjangkah" />
        <activity
            android:name=".DashboardActivity"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.SimbahNjangkah" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.SimbahNjangkah">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>